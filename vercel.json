{
  "buildCommand": "flutter/bin/flutter build web --release --dart-define=API_URL=$API_URL --dart-define=VAR_01=$VAR_01 --dart-define=VAR_02=$VAR_02 --dart-define=VAR_03=$VAR_03 --dart-define=VAR_04=$VAR_04 --dart-define=VAR_05=$VAR_05 --dart-define=VAR_06=$VAR_06 --dart-define=VAR_PRUEBA_LARGA_01=$VAR_PRUEBA_LARGA_01 --dart-define=VAR_PRUEBA_LARGA_02=$VAR_PRUEBA_LARGA_02 --dart-define=VAR_PRUEBA_LARGA_03=$VAR_PRUEBA_LARGA_03 --dart-define=VAR_PRUEBA_LARGA_04=$VAR_PRUEBA_LARGA_04 --dart-define=VAR_PRUEBA_LARGA_05=$VAR_PRUEBA_LARGA_05",
  "outputDirectory": "build/web",
  "installCommand": "if cd flutter; then git pull && cd .. ; else git clone https://github.com/flutter/flutter.git; fi && ls && flutter/bin/flutter doctor && flutter/bin/flutter clean && flutter/bin/flutter config --enable-web"
}
